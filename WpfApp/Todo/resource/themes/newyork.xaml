<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  <Color x:Key="ColorPrimary">#EE6139</Color>
  <Color x:Key="MainWindow">#FFEDA66B</Color>
  <Color x:Key="ItemHover">#14552F66</Color>
  <Color x:Key="ItemSelected">#28BF7B40</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#5D391C" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC224E47</Color>
  <Color x:Key="LeftBarBackColorBottom">#CCD63603</Color>
  <system:Double x:Key="ProjectShadowOpacity">0.13</system:Double>
</ResourceDictionary>