<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  <Color x:Key="ColorPrimary">#73B30E</Color>
  <Color x:Key="MainWindow">#FFF2F5FE</Color>
  <Color x:Key="ItemHover">#1473B30E</Color>
  <Color x:Key="ItemSelected">#2873B30E</Color>
  <Color x:Key="ProjectHoverColor">#1A73B30E</Color>
  <Color x:Key="ProjectMenuBaseColor">#191919</Color>
  <SolidColorBrush x:Key="ProjectIconTextColor" Color="#E5EFEE"/>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#3373B30E"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#191919" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC92C58A</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC92C58A</Color>
  <SolidColorBrush x:Key="ProjectMenuIconColor" Color="{DynamicResource ProjectMenuBaseColor}" Opacity="0.4"/>
  <SolidColorBrush x:Key="ProjectSectionColor" Color="{DynamicResource ProjectMenuBaseColor}" Opacity="0.4"/>
  <system:Double x:Key="ProjectShadowOpacity">0.15</system:Double>
  <SolidColorBrush x:Key="ShowAreaBackground" Color="{DynamicResource white_80}"/>
</ResourceDictionary>