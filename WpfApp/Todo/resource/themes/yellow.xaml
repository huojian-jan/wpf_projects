<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorPrimary">#FFFFA14D</Color>
  <Color x:Key="MainWindow">#FFFFA14D</Color>
  <Color x:Key="ItemHover">#14DE9566</Color>
  <Color x:Key="ItemSelected">#28DE9566</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#FFFFFF" Opacity="0.2"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#14DE9566"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <SolidColorBrush x:Key="ProjectIconTextColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#FF8E2A</Color>
  <Color x:Key="LeftBarBackColorBottom">#FF8E2A</Color>
  <SolidColorBrush x:Key="KanbanContainerColor" Color="Transparent"/>
  <SolidColorBrush x:Key="MatrixContainerColor" Color="Transparent"/>
  <SolidColorBrush x:Key="TimelineContainerBackground" Color="#FFF7EF"/>
  <Color x:Key="TimelineContainerBackgroundColor">#FFF7EF</Color>
  <Color x:Key="TimelineContainerBackgroundStepToColor">#00FFF7EF</Color>
  <SolidColorBrush x:Key="ProjectSectionColor" Color="{DynamicResource ProjectMenuBaseColor}" Opacity="0.4"/>
</ResourceDictionary>