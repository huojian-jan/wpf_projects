<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  <Color x:Key="ColorPrimary">#A6D507</Color>
  <Color x:Key="MainWindow">#FFA6D507</Color>
  <Color x:Key="ItemHover">#14A6D507</Color>
  <Color x:Key="ItemSelected">#28A6D507</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectIconTextColor" Color="#82A6A5"/>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#191919" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC7EA8B4</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC94A538</Color>
  <SolidColorBrush x:Key="ShowAreaBackground" Color="{DynamicResource white_90}"/>
  <system:Double x:Key="ProjectShadowOpacity">0.15</system:Double>
</ResourceDictionary>