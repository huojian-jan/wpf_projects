<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorPrimary">#FF00C598</Color>
  <Color x:Key="MainWindow">#FF00C598</Color>
  <Color x:Key="ItemHover">#142D9663</Color>
  <Color x:Key="ItemSelected">#287CDBCC</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#FFFFFF" Opacity="0.2"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#142D9663"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <SolidColorBrush x:Key="ProjectIconTextColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#00BB91</Color>
  <Color x:Key="LeftBarBackColorBottom">#00BB91</Color>
  <SolidColorBrush x:Key="KanbanContainerColor" Color="Transparent"/>
  <SolidColorBrush x:Key="MatrixContainerColor" Color="Transparent"/>
  <SolidColorBrush x:Key="TimelineContainerBackground" Color="#E8FAF6"/>
  <Color x:Key="TimelineContainerBackgroundColor">#E8FAF6</Color>
  <Color x:Key="TimelineContainerBackgroundStepToColor">#00E8FAF6</Color>
  <SolidColorBrush x:Key="ProjectSectionColor" Color="{DynamicResource ProjectMenuBaseColor}" Opacity="0.4"/>
</ResourceDictionary>