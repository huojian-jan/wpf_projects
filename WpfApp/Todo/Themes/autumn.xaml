<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorPrimary">#F87F00</Color>
  <Color x:Key="MainWindow">#FFF9DBC7</Color>
  <Color x:Key="ItemHover">#1A8B5024</Color>
  <Color x:Key="ItemSelected">#28E8713B</Color>
  <Color x:Key="ProjectHoverColor">#0CDC5519</Color>
  <Color x:Key="ProjectMenuBaseColor">#DC5519</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#DC5519" Opacity="0.1"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#8B5024" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CCFE8D5A</Color>
  <Color x:Key="LeftBarBackColorBottom">#CCFE8D5A</Color>
  <SolidColorBrush x:Key="ProjectSectionColor" Color="{DynamicResource ProjectMenuBaseColor}" Opacity="0.4"/>
</ResourceDictionary>