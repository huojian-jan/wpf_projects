<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorPrimary">#FF95477E</Color>
  <Color x:Key="MainWindow">#FF95477E</Color>
  <Color x:Key="ItemHover">#0F95477E</Color>
  <Color x:Key="ItemSelected">#2895477E</Color>
  <Color x:Key="ProjectHoverColor">#2895477E</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#95477E" Opacity="0.36"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#7A4B6C" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC951D65</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC951D65</Color>
</ResourceDictionary>