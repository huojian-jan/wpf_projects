<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  <Color x:Key="ColorPrimary">#278C5C</Color>
  <Color x:Key="MainWindow">#FF1C7D66</Color>
  <Color x:Key="ItemHover">#0F1C7D66</Color>
  <Color x:Key="ItemSelected">#281C7D66</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#2C5351" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC008E8A</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC869700</Color>
  <system:Double x:Key="ProjectShadowOpacity">0.13</system:Double>
</ResourceDictionary>