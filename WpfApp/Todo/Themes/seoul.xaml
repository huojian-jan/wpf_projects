<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorPrimary">#6AD3CD</Color>
  <Color x:Key="MainWindow">#FF4DA8A0</Color>
  <Color x:Key="ItemHover">#143B9E8A</Color>
  <Color x:Key="ItemSelected">#283B9E8A</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#1C5D57" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC2C8490</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC006865</Color>
</ResourceDictionary>