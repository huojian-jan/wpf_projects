<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  <Color x:Key="ColorPrimary">#64C4D8</Color>
  <Color x:Key="MainWindow">#FF64C4D8</Color>
  <Color x:Key="ItemHover">#1464C4D8</Color>
  <Color x:Key="ItemSelected">#2864C4D8</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectIconTextColor" Color="#42646E"/>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#191919" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC7797A5</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC111E26</Color>
  <system:Double x:Key="ProjectShadowOpacity">0.15</system:Double>
</ResourceDictionary>