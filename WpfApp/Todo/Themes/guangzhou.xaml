<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  <Color x:Key="ColorPrimary">#1D6DCC</Color>
  <Color x:Key="MainWindow">#FF0A4897</Color>
  <Color x:Key="ItemHover">#0F0A4897</Color>
  <Color x:Key="ItemSelected">#280A4897</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#4A6689" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC0B58AC</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC0B58AC</Color>
  <system:Double x:Key="ProjectShadowOpacity">0.13</system:Double>
</ResourceDictionary>