<UserControl x:Class="wpf_controls.customControls.PanelControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:wpf_controls.customControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
    <Button DockPanel.Dock="Left" Content="Left"/>
    <Button DockPanel.Dock="Top" Content="Left"/>
    <Button DockPanel.Dock="Right" Content="Left"/>
    <Button DockPanel.Dock="Bottom" Content="Left"/>
    <Button Content="Center"/>
            
    </DockPanel>
</UserControl>
