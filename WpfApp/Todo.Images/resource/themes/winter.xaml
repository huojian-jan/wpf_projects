<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorPrimary">#2577E1</Color>
  <Color x:Key="MainWindow">#FFC8DBF9</Color>
  <Color x:Key="ItemHover">#143B5998</Color>
  <Color x:Key="ItemSelected">#59C3DCFC</Color>
  <Color x:Key="ProjectHoverColor">#0C234283</Color>
  <Color x:Key="ProjectMenuBaseColor">#234283</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#234283" Opacity="0.1"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#3B5998" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC6E90D7</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC6E90D7</Color>
  <SolidColorBrush x:Key="ProjectSectionColor" Color="{DynamicResource ProjectMenuBaseColor}" Opacity="0.4"/>
</ResourceDictionary>