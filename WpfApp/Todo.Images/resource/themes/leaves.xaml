<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  <Color x:Key="ColorPrimary">#079349</Color>
  <Color x:Key="MainWindow">#FF6A907C</Color>
  <Color x:Key="ItemHover">#14627C6D</Color>
  <Color x:Key="ItemSelected">#2868A086</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#627C6D" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC0A0C0E</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC0A0C0E</Color>
  <system:Double x:Key="ProjectShadowOpacity">0.5</system:Double>
</ResourceDictionary>