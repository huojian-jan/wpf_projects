<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorPrimary">#FF711B</Color>
  <Color x:Key="MainWindow">#FFE7945D</Color>
  <Color x:Key="ItemHover">#14E8713B</Color>
  <Color x:Key="ItemSelected">#28E8713B</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#783B30" Opacity="0.1"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CCD63D2B</Color>
  <Color x:Key="LeftBarBackColorBottom">#CCD63D2B</Color>
</ResourceDictionary>