<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  <Color x:Key="ColorPrimary">#00BBA9</Color>
  <Color x:Key="MainWindow">#FF5B736F</Color>
  <Color x:Key="ItemHover">#1A5A7270</Color>
  <Color x:Key="ItemSelected">#285E8A86</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#5A7270" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC192B28</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC192B28</Color>
  <system:Double x:Key="ProjectShadowOpacity">0.13</system:Double>
</ResourceDictionary>