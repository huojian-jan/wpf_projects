<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  <Color x:Key="ColorPrimary">#D98317</Color>
  <Color x:Key="MainWindow">#FFBDA18D</Color>
  <Color x:Key="ItemHover">#14816A5F</Color>
  <Color x:Key="ItemSelected">#28BD9371</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#816A5F" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC763827</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC763827</Color>
  <system:Double x:Key="ProjectShadowOpacity">0.2</system:Double>
</ResourceDictionary>