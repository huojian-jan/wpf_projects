<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  <Color x:Key="ColorPrimary">#993A3A</Color>
  <Color x:Key="MainWindow">#FF993A3A</Color>
  <Color x:Key="ItemHover">#14993A3A</Color>
  <Color x:Key="ItemSelected">#28993A3A</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectIconTextColor" Color="#01212F"/>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#191919" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC113E4F</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC020202</Color>
  <system:Double x:Key="ProjectShadowOpacity">0.5</system:Double>
</ResourceDictionary>