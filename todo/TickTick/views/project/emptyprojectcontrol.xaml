<UserControl x:Class="ticktick_WPF.Views.Project.EmptyProjectControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             Height="60">
  <Border Margin="11,5" CornerRadius="4" Background="{DynamicResource ProjectSelectedBackground}">
    <TextBlock Style="{DynamicResource Tag01}" Margin="12,0" TextWrapping="Wrap"
               Foreground="{DynamicResource ProjectMenuColorOpacity100_80}" Opacity="0.6" Text="{Binding Title}"/>
  </Border>
</UserControl>