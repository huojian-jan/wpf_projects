<DockPanel x:Class="ticktick_WPF.Views.TaskList.CommentSearchLabel"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:local="clr-namespace:ticktick_WPF.Views.TaskList"
           Height="{DynamicResource Height22}">
  <Image Source="{DynamicResource CommentDrawingLine}" DockPanel.Dock="Left" Margin="0,1,2,0"
         Style="{DynamicResource Icon01}" Height="14"/>
  <TextBlock Name="CommentText" FontSize="{DynamicResource Font12}" TextWrapping="NoWrap"
             VerticalAlignment="Center" Opacity="0.4" TextTrimming="CharacterEllipsis"
             HorizontalAlignment="Left" TextAlignment="Left" LineStackingStrategy="BlockLineHeight"
             Foreground="{DynamicResource BaseColorOpacity100_80}"/>
</DockPanel>