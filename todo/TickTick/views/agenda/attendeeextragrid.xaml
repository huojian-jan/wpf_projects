<UserControl x:Class="ticktick_WPF.Views.Agenda.AttendeeExtraGrid"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:agenda="clr-namespace:ticktick_WPF.Views.Agenda"
             Height="24" Margin="4,0">
  <Grid>
    <TextBlock FontSize="14" Foreground="{DynamicResource BaseColorOpacity60}" VerticalAlignment="Center"
               Text="{Binding Title}"/>
  </Grid>
</UserControl>