<UserControl x:Class="ticktick_WPF.Views.Config.SecurityNetworkConfig"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ticktick_WPF.Views.Config">
  <Grid>
    <ScrollViewer Style="{StaticResource for_scrollviewer}">
      <Grid>
        <StackPanel Orientation="Vertical">
          <local:ProxySettings x:Name="ProxySettings" WindowMode="false"/>
        </StackPanel>
      </Grid>
    </ScrollViewer>
  </Grid>
</UserControl>