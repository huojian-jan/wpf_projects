    <Style x:Key="TagBorderStyle" TargetType="{x:Type Border}">
      <Setter Property="FrameworkElement.VerticalAlignment" Value="Center"/>
      <Setter Property="Border.CornerRadius" Value="2"/>
      <Setter Property="Border.Background" Value="{StaticResource TagBackground}"/>
    </Style>
  </FrameworkElement.Resources>
  <StackPanel Orientation="Horizontal">
    <Border Margin="5,0,0,0" Style="{StaticResource TagBorderStyle}"
            Visibility="{Binding FirstTag, Converter={StaticResource StringToVisibleConverter}}">
      <TextBlock Name="FirstTag" Text="{Binding FirstTag}"/>
    </Border>
    <Border Margin="5,0,0,0" Style="{StaticResource TagBorderStyle}"
            Visibility="{Binding SecondTag, Converter={StaticResource StringToVisibleConverter}}">
      <TextBlock Text="{Binding SecondTag}"/>
    </Border>
    <Border Margin="5,0,0,0" Style="{StaticResource TagBorderStyle}"
            Visibility="{Binding MoreTag, Converter={StaticResource StringToVisibleConverter}}">
      <TextBlock Text="{Binding MoreTag}"/>
    </Border>
    <TextBlock Opacity="0.56" Text="{Binding ProjectName}"
               Visibility="{Binding ProjectName, Converter={StaticResource StringToVisibleConverter}}"/>
    <Image Source="{StaticResource attachmentsDrawingImage}" Style="{StaticResource ListIcon}"
           Visibility="{Binding ShowAttachment, Converter={StaticResource BoolVisibleConverter}, Mode=OneWay}"/>
    <Grid Width="20" Height="20" Margin="0,0,0,1"
          Visibility="{Binding ShowProgress, Converter={StaticResource BoolVisibleConverter}}">
      <Border BorderBrush="{StaticResource BaseColorOpacity20}" BorderThickness="2" CornerRadius="6" Width="12"
              Height="12"/>
      <views:CircleProgress Radius="2" SegmentColor="{DynamicResource BaseColorOpacity20}" StrokeThickness="4"
                            HorizontalAlignment="Center" VerticalAlignment="Center" Percentage="{Binding Progress}"/>
    </Grid>
    <Image Source="{StaticResource locationDrawingImage}" Style="{StaticResource ListIcon}"
           Visibility="{Binding ShowLocation, Converter={StaticResource BoolVisibleConverter}, Mode=OneWay}"/>
    <Image Source="{StaticResource CommentDrawingImage}" Style="{StaticResource ListIcon}"
           Visibility="{Binding ShowComment, Converter={StaticResource BoolVisibleConverter}, Mode=OneWay}"/>
    <Image Source="{StaticResource HasDescDrawingImage}" Style="{StaticResource ListIcon}"
           Visibility="{Binding ShowDescription, Converter={StaticResource BoolVisibleConverter}, Mode=OneWay}"/>
    <Image Source="{StaticResource RepeatDrawingImage}" Style="{StaticResource ListIcon}"
           Visibility="{Binding ShowRepeat, Converter={StaticResource BoolVisibleConverter}, Mode=OneWay}"/>
    <Image Source="{StaticResource reminderDrawingImage}" Style="{StaticResource ListIcon}" Margin="2,2,2,0"
           Visibility="{Binding ShowReminder, Converter={StaticResource BoolVisibleConverter}, Mode=OneWay}"/>
    <TextBlock Name="DateText" FontSize="12" Style="{StaticResource TaskListViewTitleStyle}"
               VerticalAlignment="Center" Margin="8,0,0,0">
      <TextBlock.Text>
        <MultiBinding Converter="{StaticResource TaskListDateConverter}">
          <Binding Path="StartDate"/>
          <Binding Path="DueDate"/>
          <Binding Path="IsAllDay"/>
          <Binding Path="Status"/>
          <Binding Source="{x:Static resource:LocalSettings.Settings}" Path="ShowCountDown"/>
          <Binding/>
          <Binding Path="IsNote"/>
        </MultiBinding>
      </TextBlock.Text>
      <TextBlock.Foreground>
        <MultiBinding Converter="{StaticResource TaskListPrintDateColorConverter}">
          <Binding Path="StartDate"/>
          <Binding Path="DueDate"/>
          <Binding Path="IsAllDay"/>
          <Binding Path="Status"/>
          <Binding Source="{x:Static resource:LocalSettings.Settings}" Path="ThemeId"/>
        </MultiBinding>
      </TextBlock.Foreground>
    </TextBlock>
    <Border HorizontalAlignment="Center" Margin="10,0,5,0" VerticalAlignment="Center" BorderBrush="#14191919"
            BorderThickness="1" CornerRadius="8"
            Visibility="{Binding AvatarUrl, Converter={StaticResource TaskListAvatarVisibilityConverter}, Mode=OneWay}">
      <Rectangle Name="AvatarImage" RadiusX="8" RadiusY="8" Width="16" Height="16"/>
    </Border>
  </StackPanel>
</UserControl>