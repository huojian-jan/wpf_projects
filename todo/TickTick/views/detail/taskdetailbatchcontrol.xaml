            <Style TargetType="{x:Type Path}" BasedOn="{StaticResource BatchTaskIconStyle}">
              <Setter Property="Shape.Fill" Value="#FF4772FA"/>
            </Style>
          </FrameworkElement.Style>
        </Path>
        <TextBlock IsEnabled="{Binding ElementName=BatchCompleteOrUndoneBorder, Path=Tag}">
          <FrameworkElement.Style>
            <Style TargetType="{x:Type TextBlock}" BasedOn="{StaticResource BatchTaskTextStyle}">
              <Setter Property="TextBlock.Text" Value="{StaticResource TaskDone}"/>
              <Style.Triggers>
                <DataTrigger Binding="{Binding ElementName=BatchTaskUndoneIcon, Path=Visibility}" Value="Visible">
                  <Setter Property="TextBlock.Text" Value="{StaticResource TaskUndone}"/>
                </DataTrigger>
              </Style.Triggers>
            </Style>
          </FrameworkElement.Style>
        </TextBlock>
      </Grid>
    </Border>
    <Border MouseLeftButtonUp="OnStarMouseUp" Name="BatchTaskStarBorder"
            Style="{StaticResource BatchTaskBorderStyle}">
      <Grid>
        <Path Name="BatchTaskStartPinIcon" IsEnabled="{Binding ElementName=BatchTaskStarBorder, Path=Tag}"
              Data="{Binding Source={StaticResource IcTaskPinned}, Path=Data}">
          <FrameworkElement.Style>
            <Style TargetType="{x:Type Path}" BasedOn="{StaticResource BatchTaskIconStyle}">
              <Setter Property="Shape.Fill" Value="#FFFFB000"/>
            </Style>
          </FrameworkElement.Style>
        </Path>
        <Path Name="BatchTaskStartUnPinIcon" Visibility="Collapsed"
              IsEnabled="{Binding ElementName=BatchTaskStarBorder, Path=Tag}"
              Data="{Binding Source={StaticResource IcTaskUnPinned}, Path=Data}">
          <FrameworkElement.Style>
            <Style TargetType="{x:Type Path}" BasedOn="{StaticResource BatchTaskIconStyle}">
              <Setter Property="Shape.Fill" Value="#FFFFB000"/>
            </Style>
          </FrameworkElement.Style>
        </Path>
        <TextBlock IsEnabled="{Binding ElementName=BatchTaskStarBorder, Path=Tag}">
          <FrameworkElement.Style>
            <Style TargetType="{x:Type TextBlock}" BasedOn="{StaticResource BatchTaskTextStyle}">
              <Setter Property="TextBlock.Text" Value="{StaticResource Pin}"/>
              <Style.Triggers>
                <DataTrigger Binding="{Binding ElementName=BatchTaskStartUnPinIcon, Path=Visibility}" Value="Visible">
                  <Setter Property="TextBlock.Text" Value="{StaticResource Unpin}"/>
                </DataTrigger>
              </Style.Triggers>
            </Style>
          </FrameworkElement.Style>
        </TextBlock>
      </Grid>
    </Border>
    <Border MouseLeftButtonUp="OnMergeMouseUp" Name="BatchTaskMergeBorder"
            Style="{DynamicResource BatchTaskBorderStyle}">
