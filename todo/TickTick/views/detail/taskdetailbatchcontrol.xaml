<Grid x:Class="ticktick_WPF.Views.Detail.TaskDetailBatchControl"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:ticktick_WPF.Views.Detail"
      xmlns:resource="clr-namespace:ticktick_WPF.Resource"
      xmlns:misc="clr-namespace:ticktick_WPF.Views.Misc"
      xmlns:views="clr-namespace:ticktick_WPF.Views"
      xmlns:wpf="clr-namespace:Emoji.Wpf;assembly=Emoji.Wpf"
      Background="Transparent">
  <FrameworkElement.Resources>
    <Style x:Key="BatchTaskIconStyle" TargetType="{x:Type Path}">
      <Setter Property="FrameworkElement.HorizontalAlignment" Value="Center"/>
      <Setter Property="FrameworkElement.VerticalAlignment" Value="Top"/>
      <Setter Property="FrameworkElement.Margin" Value="0,9,0,0"/>
      <Setter Property="FrameworkElement.Width" Value="20"/>
      <Setter Property="FrameworkElement.Height" Value="20"/>
      <Setter Property="Shape.Stretch" Value="Fill"/>
      <Style.Triggers>
        <Trigger Property="UIElement.IsEnabled" Value="false">
          <Setter Property="Shape.Fill" Value="{DynamicResource BaseColorOpacity20}"/>
        </Trigger>
      </Style.Triggers>
    </Style>
    <Style x:Key="BatchTaskTextStyle" TargetType="{x:Type TextBlock}">
      <Setter Property="FrameworkElement.HorizontalAlignment" Value="Center"/>
      <Setter Property="TextBlock.TextAlignment" Value="Center"/>
      <Setter Property="FrameworkElement.VerticalAlignment" Value="Bottom"/>
      <Setter Property="TextBlock.FontSize" Value="14"/>
      <Setter Property="FrameworkElement.Margin" Value="0,0,0,9"/>
      <Setter Property="TextBlock.Foreground" Value="{DynamicResource BaseColorOpacity80}"/>
      <Style.Triggers>
        <Trigger Property="UIElement.IsEnabled" Value="false">
          <Setter Property="TextBlock.Foreground" Value="{DynamicResource BaseColorOpacity20}"/>
        </Trigger>
      </Style.Triggers>
    </Style>
    <Style x:Key="BatchTaskBorderStyle" TargetType="{x:Type Border}">
      <Setter Property="FrameworkElement.Cursor" Value="Hand"/>
      <Setter Property="Border.Background" Value="{DynamicResource BatchSelectBackground}"/>
      <Setter Property="FrameworkElement.Margin" Value="6,6"/>
      <Setter Property="Border.CornerRadius" Value="6"/>
      <Setter Property="FrameworkElement.Height" Value="67"/>
      <Setter Property="FrameworkElement.Width"
              Value="{Binding ElementName=BatchBlockWidthTarget, Path=ActualWidth, Mode=OneWay}"/>
      <Style.Triggers>
        <Trigger Property="FrameworkElement.Tag" Value="False">
          <Setter Property="FrameworkElement.Cursor" Value="Arrow"/>
        </Trigger>
        <Trigger Property="UIElement.IsMouseOver" Value="true">
          <Setter Property="Border.Background" Value="{DynamicResource BaseColorOpacity5}"/>
        </Trigger>
      </Style.Triggers>
    </Style>
    <Style x:Key="DropIconStyle" TargetType="{x:Type misc:DropIconButton}">
      <Setter Property="Control.FontSize" Value="14"/>
      <Setter Property="misc:DropIconButton.IconSize" Value="18"/>
      <Setter Property="Control.Foreground" Value="{DynamicResource BaseColorOpacity80}"/>
      <Setter Property="misc:DropIconButton.IconColor" Value="{DynamicResource BaseColorOpacity60}"/>
      <Style.Triggers>
        <Trigger Property="misc:DropIconButton.ItemSelected" Value="true">
          <Setter Property="Control.Foreground" Value="{DynamicResource PrimaryColor}"/>
          <Setter Property="misc:DropIconButton.IconColor" Value="{DynamicResource PrimaryColor}"/>
        </Trigger>
      </Style.Triggers>
    </Style>
  </FrameworkElement.Resources>
  <Grid.RowDefinitions>
    <RowDefinition Height="Auto"/>
    <RowDefinition Height="Auto"/>
    <RowDefinition Height="Auto"/>
    <RowDefinition Height="Auto"/>
  </Grid.RowDefinitions>
  <TextBlock Name="BatchTaskText" Foreground="{DynamicResource BaseColorOpacity100_80}" Margin="20, 0"
             FontSize="16" FontWeight="SemiBold"/>
  <TextBlock MouseLeftButtonUp="OnCancelClick" Name="CloseButton" Grid.Row="0" HorizontalAlignment="Right"
             VerticalAlignment="Center" Foreground="{DynamicResource PrimaryColor}" Margin="20, 0" FontSize="14"
             Text="{StaticResource Cancel}"/>
  <Border Grid.Row="1" CornerRadius="6" Margin="20,20" VerticalAlignment="Top"
          Background="{DynamicResource BatchSelectBackground}">
    <StackPanel Orientation="Vertical">
      <misc:OptionItemWithImageIcon x:Name="BatchRestoreProjectButton" MouseLeftButtonUp="RestoreProjectClick"
                                    ImageSource="{DynamicResource RestoreDrawingImage}" CustomFontSize="14" Height="44"
                                    Visibility="Collapsed" Content="{StaticResource Restore}"/>
      <misc:EscPopup x:Name="SetProjectPopup" Placement="Bottom" HorizontalOffset="24" VerticalOffset="-15"
                     StaysOpen="false" PlacementTarget="{Binding ElementName=BatchTaskMoveProjectButton}"/>
      <misc:OptionItemWithImageIcon x:Name="BatchDeleteFromTrashButton" MouseLeftButtonUp="DeleteFromTrashClick" Height="44"
                                    Visibility="Collapsed" ImageSource="{DynamicResource DeleteForeverDrawingImage}"
                                    CustomFontSize="14" Content="{StaticResource DeleteForever}"/>
      <Grid MouseLeftButtonUp="SelectDateClick" Name="BatchSelectDateButton" Visibility="Collapsed"
            Cursor="Hand" MinWidth="120" IsEnabled="{Binding Enable}">
        <FrameworkElement.Style>
          <Style TargetType="{x:Type Grid}">
            <Setter Property="Panel.Background" Value="Transparent"/>
            <Style.Triggers>
              <Trigger Property="UIElement.IsMouseOver" Value="true">
                <Setter Property="Panel.Background" Value="{DynamicResource BaseColorOpacity5}"/>
              </Trigger>
            </Style.Triggers>
          </Style>
        </FrameworkElement.Style>
        <Grid.ColumnDefinitions>
          <ColumnDefinition Width="Auto"/>
          <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
          <RowDefinition Height="44"/>
          <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Path Width="18" Height="18" Stretch="Uniform" Grid.Column="0" HorizontalAlignment="Left"
              Margin="16,0,0,0" IsHitTestVisible="false"
              Data="{Binding Source={StaticResource IcCalendar}, Path=Data}">
          <Shape.Fill>
            <MultiBinding Converter="{StaticResource TaskDetailDateColorConverter}">
              <Binding Path="DisplayStartDate"/>
              <Binding Path="DisplayDueDate"/>
              <Binding Path="IsAllDay"/>
              <Binding Source="{x:Static resource:LocalSettings.Settings}" Path="ThemeId"/>
            </MultiBinding>
          </Shape.Fill>
        </Path>
        <TextBlock Grid.Row="0" Margin="8,0" Background="Transparent" TextTrimming="CharacterEllipsis" Grid.Column="1"
                   Style="{StaticResource Tag01}" IsHitTestVisible="false" FontSize="14"
                   Foreground="{Binding DateIconColor}" Cursor="{Binding ElementName=ChooseTimeGrid, Path=Cursor}">
          <FrameworkElement.MaxWidth>
            <MultiBinding Converter="{StaticResource TaskDetailDateMaxWidthConverter}">
              <Binding ElementName="DateGrid" Path="ActualWidth"/>
              <Binding ElementName="RepeatIndicator" Path="Visibility"/>
            </MultiBinding>
          </FrameworkElement.MaxWidth>
          <TextBlock.Text>
            <MultiBinding Converter="{StaticResource TaskDetailDateConverter}">
              <Binding Path="DisplayStartDate"/>
              <Binding Path="DisplayDueDate"/>
              <Binding Path="IsAllDay"/>
              <Binding Path="Mode"/>
              <Binding Path="Kind"/>
              <Binding Path="Status"/>
            </MultiBinding>
          </TextBlock.Text>
        </TextBlock>
        <TextBlock Grid.Row="1" Grid.Column="1" Foreground="{DynamicResource BaseColorOpacity40}" FontSize="12"
                   Margin="5,0,5, 11" Visibility="{Binding ShowTimeZoneText}">
          <TextBlock.Text>
            <MultiBinding Converter="{StaticResource TaskDetailTimeZoneDateConverter}">
              <Binding Path="DisplayStartDate"/>
              <Binding Path="DisplayDueDate"/>
              <Binding Path="IsAllDay"/>
              <Binding Path="TimeZoneName"/>
            </MultiBinding>
          </TextBlock.Text>
        </TextBlock>
      </Grid>
      <Grid MouseLeftButtonUp="SetPriorityClick" Name="BatchTaskPriorityButton" Height="44">
        <FrameworkElement.Style>
          <Style TargetType="{x:Type Grid}">
            <Setter Property="FrameworkElement.Cursor" Value="Hand"/>
            <Setter Property="Panel.Background" Value="Transparent"/>
            <Style.Triggers>
              <Trigger Property="UIElement.IsMouseOver" Value="true">
                <Setter Property="Panel.Background" Value="{DynamicResource BaseColorOpacity5}"/>
              </Trigger>
            </Style.Triggers>
          </Style>
        </FrameworkElement.Style>
        <Grid.ColumnDefinitions>
          <ColumnDefinition Width="Auto"/>
          <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Image Width="18" Height="18" IsHitTestVisible="false" Margin="16,13,0,0" VerticalAlignment="Top">
          <Image.Source>
            <MultiBinding Converter="{StaticResource TaskDetailPriorityIconConverter}">
              <Binding Path="Priority"/>
              <Binding Source="{x:Static resource:LocalSettings.Settings}" Path="ThemeId"/>
            </MultiBinding>
          </Image.Source>
        </Image>
        <TextBlock Name="PriorityText" Grid.Column="1" Text="{StaticResource priority}" IsHitTestVisible="false"
                   VerticalAlignment="Center" Margin="8,0,12,0" Foreground="{DynamicResource BaseColorOpacity80}"
                   FontSize="14" Tag="{Binding Priority, Converter={StaticResource TaskDetailPriorityTextConverter}}"/>
      </Grid>
      <misc:EscPopup x:Name="SetPriorityPopup" StaysOpen="false" HorizontalOffset="24" VerticalOffset="-12"
                     Placement="Bottom" PlacementTarget="{Binding ElementName=BatchTaskPriorityButton}"/>
      <misc:DropIconButton x:Name="BatchTaskMoveProjectButton" MouseLeftButtonUp="MoveProjectMouseUp" Visibility="Collapsed"
                           Height="44" Margin="0,0,0,0" IconSize="18" Title="{StaticResource MoveTo}" FontSize="14"
                           Style="{DynamicResource DropIconStyle}"
                           Icon="{Binding Source={StaticResource IcMovetoLine}, Path=Data}"/>
      <Grid MouseLeftButtonUp="OnSetAssigneeMouseUp" Name="BatchAssigneeGrid" Visibility="Collapsed">
        <Grid.ColumnDefinitions>
          <ColumnDefinition Width="auto"/>
          <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <FrameworkElement.Style>
          <Style TargetType="{x:Type Grid}">
            <Setter Property="FrameworkElement.Cursor" Value="Hand"/>
            <Setter Property="FrameworkElement.Height" Value="44"/>
            <Style.Triggers>
              <Trigger Property="UIElement.IsMouseOver" Value="true">
                <Setter Property="Panel.Background" Value="{DynamicResource BaseColorOpacity5}"/>
              </Trigger>
            </Style.Triggers>
          </Style>
        </FrameworkElement.Style>
        <Rectangle Name="AvatarImage" RadiusX="18" RadiusY="18" Width="18" Height="18" Margin="16,0,0,0"
                   HorizontalAlignment="Left" Visibility="{Binding ElementName=AvatarImage, Path=Visibility}">
          <Shape.Fill>
            <ImageBrush x:Name="Avatar" Stretch="Fill"/>
          </Shape.Fill>
        </Rectangle>
        <Image Margin="16,0,0,0" Stretch="Uniform" Style="{DynamicResource Icon01}"
               Source="{DynamicResource AssigntoDrawingImage}" Height="18" Width="18"
               Visibility="{Binding ElementName=AvatarImage, Path=Visibility, Converter={StaticResource InverseVisibleConverter}}"/>
        <wpf:EmjTextBlock Grid.Column="1" VerticalAlignment="Center" Margin="8,0,16,0" FontSize="14"
                          Width="{Binding ElementName=BatchTaskMoveProjectButton, Path=ActualWidth}"
                          MaxWidth="{Binding ElementName=BatchTaskMoveProjectButton, Path=ActualWidth}">
          <wpf:EmjTextBlock.Text>
            <MultiBinding Converter="{StaticResource AvatarNameConverter}">
              <Binding Path="Assignee"/>
              <Binding Path="ProjectId"/>
            </MultiBinding>
          </wpf:EmjTextBlock.Text>
          <FrameworkElement.Style>
            <Style TargetType="{x:Type wpf:EmjTextBlock}">
              <Setter Property="TextBlock.Foreground" Value="{DynamicResource BaseColorOpacity80}"/>
              <Style.Triggers>
                <DataTrigger Binding="{Binding ElementName=AvatarImage, Path=Visibility}" Value="Visible">
                  <Setter Property="TextBlock.Foreground" Value="{DynamicResource PrimaryColor}"/>
                </DataTrigger>
              </Style.Triggers>
            </Style>
          </FrameworkElement.Style>
        </wpf:EmjTextBlock>
      </Grid>
      <misc:EscPopup x:Name="BatchAssigneePopup" Placement="Bottom" StaysOpen="false" VerticalOffset="-12"
                     HorizontalOffset="24" PlacementTarget="{Binding ElementName=BatchAssigneeGrid}"/>
      <Grid MouseLeftButtonUp="OnTagMouseUp" Name="BatchSetTagsButton" MinHeight="44" Visibility="Collapsed">
        <FrameworkElement.Style>
          <Style TargetType="{x:Type Grid}">
            <Setter Property="FrameworkElement.Cursor" Value="Hand"/>
            <Setter Property="Panel.Background" Value="Transparent"/>
            <Style.Triggers>
              <Trigger Property="UIElement.IsMouseOver" Value="true">
                <Setter Property="Panel.Background" Value="{DynamicResource BaseColorOpacity5}"/>
              </Trigger>
            </Style.Triggers>
          </Style>
        </FrameworkElement.Style>
        <Grid.ColumnDefinitions>
          <ColumnDefinition Width="Auto"/>
          <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Path Width="18" Height="18" IsHitTestVisible="false" Margin="16,13,0,0" VerticalAlignment="Top"
              Data="{Binding Source={StaticResource IcTagLine}, Path=Data}">
          <FrameworkElement.Style>
            <Style TargetType="{x:Type Path}">
              <Setter Property="Shape.Stretch" Value="Uniform"/>
              <Setter Property="FrameworkElement.VerticalAlignment" Value="Center"/>
              <Setter Property="Shape.Fill" Value="{DynamicResource PrimaryColor}"/>
              <Style.Triggers>
                <DataTrigger Binding="{Binding ElementName=BatchTagsText, Path=Visibility}" Value="Visible">
                  <Setter Property="Shape.Fill" Value="{DynamicResource BaseColorOpacity60}"/>
                </DataTrigger>
              </Style.Triggers>
            </Style>
          </FrameworkElement.Style>
        </Path>
        <TextBlock Name="BatchTagsText" Grid.Column="1" Visibility="Collapsed" Text="{StaticResource Tags}"
                   IsHitTestVisible="false" VerticalAlignment="Center" Margin="8,0,12,0"
                   Foreground="{DynamicResource BaseColorOpacity80}" FontSize="14"/>
        <Border Name="BatchTagsContainer" Grid.Column="1" Margin="6,8,0,8" IsHitTestVisible="false"/>
      </Grid>
    </StackPanel>
  </Border>
  <Grid Row="2" Margin="15,0">
    <Grid.ColumnDefinitions>
      <ColumnDefinition/>
      <ColumnDefinition/>
    </Grid.ColumnDefinitions>
    <Border Name="BatchBlockWidthTarget" Background="Transparent" Grid.Column="0" Width="Auto" Height="0"
            Style="{DynamicResource BatchTaskBorderStyle}"/>
    <Border Background="Transparent" Grid.Column="1" Width="Auto" Height="0"
            Style="{DynamicResource BatchTaskBorderStyle}"/>
  </Grid>
  <WrapPanel Name="BatchTaskBlocks" Grid.Row="3" Margin="15 0 14 0" Visibility="Collapsed">
    <Border MouseLeftButtonUp="OnBatchCompleteMouseUp" Name="BatchCompleteOrUndoneBorder"
            Style="{DynamicResource BatchTaskBorderStyle}">
