                  Height="40" Width="270" Tag="8"
                  Visibility="{Binding ElementName=ThirdLoginEnGrid, Path=Visibility}">
            <ContentControl.Content>
              <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
                <Path Fill="White" Width="24" Height="24" Stretch="Uniform" Margin="-4,0,0,0"
                      Data="F1 M 24,24 0,0 6.39405095,6 L 17.5989542,6 C 18.3630574,6 18.9906697,6.68477649 19,7.52353564 L 11.999419,11.8328492 5,7.52820648 C 5.00699942,6.68710963 5.62878114,6 6.39405095,6 M 5.00583285,9.50320693 L 5,16.2926434 C 5,17.047414 5.62994771,17.6656983 6.3998838,17.6656983 L 17.5989542,17.6656983 C 18.3688903,17.6656983 18.998838,17.0474163 18.998838,16.2914813 L 18.998838,9.4997095 12.1674096,13.1592413 12.1674096,13.1592413 C 12.0626853,13.2162946 11.9361622,13.2162946 11.8314375,13.1592414 L 5.007004,9.50320694 5.00583285,9.50320693 Z"/>
                <TextBlock Text="{StaticResource SignInWithEmail}" VerticalAlignment="Center" Margin="8,0,0,0"/>
              </StackPanel>
            </ContentControl.Content>
          </Button>
          <Button Click="GoogleLoginClick" Style="{DynamicResource SaveButtonStyle}" Background="Transparent"
                  BorderThickness="1" BorderBrush="#66191919" Height="40" Width="270" Tag="8" Margin="0,14,0,0"
                  Visibility="{Binding ElementName=ThirdLoginEnGrid, Path=Visibility}">
            <ContentControl.Content>
              <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
                <Path Fill="#191919" Width="24" Height="24" Stretch="Uniform" Margin="-4,0,0,0"
                      Data="F1 M 25,24 0,0 18.872731,10.6017018 L 12.1470493,10.6017018 C 12.1470493,11.3015646 12.1470493,12.6998859 12.1421503,13.39975 L 16.0396783,13.39975 C 15.8906082,14.0996127 15.3608115,15.0780156 14.612661,15.5714205 14.6119611,15.5707206 14.6112613,15.5756197 14.6098615,15.5749198 13.6153572,16.2313907 12.3024155,16.3804608 11.327514,16.1845002 9.79972117,15.88146 8.59105765,14.7735747 8.09974458,13.3661493 8.10254402,13.3640497 8.10464361,13.3444535 8.1067432,13.3430539 7.79950437,12.4696251 7.79950437,11.3008562 8.10744307,10.6016894 8.5028656,9.31604218 9.74861685,8.14307323 11.2778179,7.82183848 12.5088759,7.56079013 13.8974068,7.84353427 14.9185057,8.79884685 15.0542789,8.66587308 16.7976387,6.96380565 16.9285061,6.82524104 13.4389883,3.66536627 7.85339199,4.77743787 5.76290518,8.85624145 L 5.75870601,8.86393992 5.75870564,8.86394065 C 4.74460817,10.8327886 4.74721677,13.1709281 5.76570488,15.1375065 L 5.75870626,15.1431054 5.75870627,15.1431055 C 6.68941204,16.9366251 8.33790849,18.2505895 10.2938092,18.7578987 12.4003878,19.31079 15.0829632,18.9328648 16.879518,17.3077896 L 16.8816176,17.3098892 C 18.4038186,15.9395556 19.3514311,13.6034122 18.8727235,10.6010103 "/>
                <TextBlock Text="{StaticResource SignInWithGoogle}" Foreground="#191919" VerticalAlignment="Center"
                           Margin="8,0,0,0"/>
              </StackPanel>
            </ContentControl.Content>
          </Button>
          <Border MouseLeftButtonUp="ShowMoreLoginType" Margin="0,20,0,0" Background="Transparent" Cursor="Hand"
                  HorizontalAlignment="Center">
            <TextBlock Text="{StaticResource MoreLoginType}" HorizontalAlignment="Center" VerticalAlignment="Center"
                       FontSize="13" Foreground="#66191919" Margin="10,4"/>
          </Border>
        </StackPanel>
        <Grid Name="signGrid" Row="1" Opacity="0" IsHitTestVisible="false">
          <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="37"/>
          </Grid.RowDefinitions>
          <Grid>
            <StackPanel Width="240" VerticalAlignment="Top" Margin="0,40,0,20">
              <TextBox GotFocus="TextBlock_GotFocus" Name="nameTextBlock" MaxLength="40" Height="36"
                       Style="{StaticResource TextBoxStyle}" Cursor="IBeam" Tag="{DynamicResource LoginNickname}"/>
              <Grid Margin="0,12,0,0" Height="40">
                <TextBox TextChanged="OnTextChanged" GotFocus="TextBlock_GotFocus" KeyDown="Login_KeyDown"
                         Name="emailTextBlock" Height="36" Padding="8,0,20,0" VerticalAlignment="Center" Cursor="IBeam">
                  <FrameworkElement.Style>
                    <Style BasedOn="{StaticResource TextBoxStyle}" TargetType="{x:Type TextBox}">
                      <Style.Triggers>
                        <DataTrigger Binding="{Binding ElementName=AccountRemainderTimesText, Path=IsVisible}" Value="True">
                          <Setter Property="Control.BorderBrush" Value="Red"/>
                        </DataTrigger>
                      </Style.Triggers>
                    </Style>
                  </FrameworkElement.Style>
                </TextBox>
                <Border MouseLeftButtonUp="ClearEmailClick" HorizontalAlignment="Right" Background="Transparent"
                        Cursor="Hand" Margin="0,0,8,0">
                  <FrameworkElement.Style>
                    <Style BasedOn="{StaticResource SmoothHoverBorderStyle40_60}" TargetType="{x:Type Border}">
                      <Setter Property="UIElement.Visibility" Value="Visible"/>
                      <Style.Triggers>
                        <DataTrigger Binding="{Binding ElementName=emailTextBlock, Path=Text}" Value="">
                          <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding ElementName=emailTextBlock, Path=Text}" Value="{x:Null}">
                          <Setter Property="UIElement.Visibility" Value="Collapsed"/>
                        </DataTrigger>
                      </Style.Triggers>
                    </Style>
                  </FrameworkElement.Style>
                  <Path Width="14" Height="14" Fill="#191919" Stretch="Uniform" VerticalAlignment="Center"
                        HorizontalAlignment="Right" Data="{Binding Source={StaticResource IcX}, Path=Data}"/>
                </Border>
              </Grid>
              <TextBlock Name="AccountRemainderTimesText" VerticalAlignment="Top" TextWrapping="Wrap" Foreground="Red"
                         FontSize="12" Margin="10,2,0,0"
                         Visibility="{Binding ElementName=AccountRemainderTimesText, Path=Text, Converter={StaticResource StringToVisibleConverter}}"/>
              <PasswordBox KeyDown="Login_KeyDown" PasswordChanged="OnPasswordChanged" Name="passwordPasswordBox" Height="36"
                           FontSize="14" Margin="0,12,0,0" BorderThickness="1" Padding="8,0" Tag="{DynamicResource Password}"
                           Cursor="IBeam">
                <FrameworkElement.Style>
                  <Style TargetType="{x:Type PasswordBox}">
                    <Setter Property="local:PasswordBoxMonitor.IsMonitoring" Value="true"/>
                    <Setter Property="Control.Template">
                      <Setter.Value>
                        <ControlTemplate TargetType="{x:Type PasswordBox}">
                          <Border x:Name="border" BorderBrush="#1A191919" BorderThickness="1" Background="Transparent"
                                  CornerRadius="4">
                            <Grid>
                              <ScrollViewer x:Name="PART_ContentHost" Opacity="0.85" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                              <TextBlock x:Name="hintText" Text="{TemplateBinding FrameworkElement.Tag}" Visibility="Collapsed"
                                         Style="{DynamicResource Body05}" Foreground="#66191919" Padding="10,0"
                                         FontSize="{TemplateBinding Control.FontSize}"/>
                            </Grid>
                          </Border>
                          <ControlTemplate.Triggers>
                            <Trigger Property="UIElement.IsFocused" Value="true">
                              <Setter TargetName="border" Property="Border.BorderBrush" Value="#894667CF"/>
                              <Setter TargetName="border" Property="Border.Background" Value="#0F2C4276"/>
                            </Trigger>
                            <Trigger Property="UIElement.IsFocused" Value="false">
                              <Setter TargetName="border" Property="Border.BorderBrush" Value="#1A191919"/>
                              <Setter TargetName="border" Property="Border.Background" Value="Transparent"/>
                            </Trigger>
                            <Trigger Property="local:PasswordBoxMonitor.PasswordLength" Value="0">
                              <Setter TargetName="hintText" Property="UIElement.Visibility" Value="Visible"/>
                            </Trigger>
                            <DataTrigger Value="True" Binding="{Binding ElementName=PasswordRemainderTimesText, Path=IsVisible}">
                              <Setter TargetName="border" Property="Border.BorderBrush" Value="Red"/>
                            </DataTrigger>
                          </ControlTemplate.Triggers>
                        </ControlTemplate>
                      </Setter.Value>
                    </Setter>
                  </Style>
                </FrameworkElement.Style>
              </PasswordBox>
              <TextBlock Name="PasswordRemainderTimesText" VerticalAlignment="Top" TextWrapping="Wrap" Foreground="Red"
                         FontSize="12" Margin="10,4,0,0"
                         Visibility="{Binding ElementName=PasswordRemainderTimesText, Path=Text, Converter={StaticResource StringToVisibleConverter}}"/>
              <Grid Name="signInButton" Margin="0,14,0,0">
                <Button Click="signInButton_Click" IsEnabled="true" Height="36" Style="{StaticResource SaveButtonStyle}"
                        Background="#4B6FDE"/>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                  <local:LoadingIndicator x:Name="SignInIndicator" SpeedRatio="2" IsActive="true" Foreground="White"
                                          Style="{StaticResource LoadingIndicatorRingStyle}" IsHitTestVisible="false" Visibility="Collapsed"
                                          Margin="0,0,8,0"/>
                  <TextBlock Text="{StaticResource PublicSignIn}" Foreground="White" VerticalAlignment="Center" FontSize="13"
                             IsHitTestVisible="false"/>
                </StackPanel>
              </Grid>
              <Grid Name="signUpButton" Margin="0,14,0,0" Visibility="Collapsed">
                <Button Click="signUpButton_Click" IsEnabled="true" Height="36" Style="{StaticResource SaveButtonStyle}"
                        Background="#4B6FDE"/>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                  <local:LoadingIndicator x:Name="SignUpIndicator" SpeedRatio="2" IsActive="true" Foreground="White"
                                          Style="{StaticResource LoadingIndicatorRingStyle}" Visibility="Collapsed" Margin="0,0,8,0"/>
                  <TextBlock Text="{StaticResource LoginCreateAccount}" Foreground="White" VerticalAlignment="Center"
                             FontSize="13" IsHitTestVisible="false"/>
                </StackPanel>
              </Grid>
              <Grid Name="ForgetAndSignupGrid" Margin="0,8,0,0">
                <TextBlock MouseLeftButtonUp="forgetPasswordButton_Click" Text="{StaticResource LoginForgetPasssword}"
                           FontSize="12" Foreground="#66191919" HorizontalAlignment="Left" Cursor="Hand"
                           Background="Transparent"/>
                <TextBlock MouseLeftButtonUp="SignUpClick" Text="{StaticResource PublicSignUp}" FontSize="12"
                           HorizontalAlignment="Right" Foreground="#5B191919" Cursor="Hand" Background="Transparent"/>
              </Grid>
            </StackPanel>
          </Grid>
        </Grid>
      </Grid>
    </Grid>
  </Border>
</Window>