<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorPrimary">#004968</Color>
  <Color x:Key="MainWindow">#FF4F9AAE</Color>
  <Color x:Key="ItemHover">#14006784</Color>
  <Color x:Key="ItemSelected">#28219CB2</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#006784" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC007685</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC00334A</Color>
</ResourceDictionary>