<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  <Color x:Key="ColorPrimary">#484553</Color>
  <Color x:Key="MainWindow">#FF434343</Color>
  <Color x:Key="ItemHover">#145D626F</Color>
  <Color x:Key="ItemSelected">#285D626F</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#5D626F" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <SolidColorBrush x:Key="DateColorPrimary" Color="{DynamicResource primary_blue_100}"/>
  <SolidColorBrush x:Key="TextAccentColor" Color="{DynamicResource primary_blue_100}"/>
  <Color x:Key="LeftBarBackColorTop">#CC0B0B0B</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC0B0B0B</Color>
  <system:Double x:Key="ProjectShadowOpacity">1</system:Double>
</ResourceDictionary>