<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorPrimary">#FF5850</Color>
  <Color x:Key="MainWindow">#FFCA8D90</Color>
  <Color x:Key="ItemHover">#145D1C1C</Color>
  <Color x:Key="ItemSelected">#28BF4047</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#5D1C1C" Opacity="0.08"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <SolidColorBrush x:Key="DateColorPrimary" Color="{DynamicResource primary_blue_100}"/>
  <Color x:Key="LeftBarBackColorTop">#CCC16DAF</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC228D9C</Color>
</ResourceDictionary>