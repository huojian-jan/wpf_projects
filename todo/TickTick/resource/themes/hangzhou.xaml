<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorPrimary">#10CDD8</Color>
  <Color x:Key="MainWindow">#FF75BCB4</Color>
  <Color x:Key="ItemHover">#1E6CC4AE</Color>
  <Color x:Key="ItemSelected">#286CC4AE</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#0C6B62" Opacity="0.10"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC26A4C9</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC079585</Color>
</ResourceDictionary>