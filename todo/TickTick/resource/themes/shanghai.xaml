<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorPrimary">#F79621</Color>
  <Color x:Key="MainWindow">#FF8598F8</Color>
  <Color x:Key="ItemHover">#145D1C4E</Color>
  <Color x:Key="ItemSelected">#286B2EC7</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#33FFFFFF"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#5D1C4E" Opacity="0.08"/>
  <SolidColorBrush x:Key="KanbanContainerColor" Color="#556FEA" Opacity="0.04"/>
  <SolidColorBrush x:Key="MatrixContainerColor" Color="#556FEA" Opacity="0.04"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#CC2E31C7</Color>
  <Color x:Key="LeftBarBackColorBottom">#CC2E31C7</Color>
</ResourceDictionary>