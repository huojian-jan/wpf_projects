<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="ColorPrimary">#FFFF739F</Color>
  <Color x:Key="MainWindow">#FFFF739F</Color>
  <Color x:Key="ItemHover">#14DE66D8</Color>
  <Color x:Key="ItemSelected">#28E27294</Color>
  <Color x:Key="ProjectHoverColor">#1AFFFFFF</Color>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="#FFFFFF" Opacity="0.2"/>
  <SolidColorBrush x:Key="TextBoxBackground" Color="#14DE66D8"/>
  <SolidColorBrush x:Key="PrimaryColor" Color="{DynamicResource ColorPrimary}"/>
  <SolidColorBrush x:Key="ProjectIconTextColor" Color="{DynamicResource ColorPrimary}"/>
  <Color x:Key="LeftBarBackColorTop">#FF5C8F</Color>
  <Color x:Key="LeftBarBackColorBottom">#FF5C8F</Color>
  <SolidColorBrush x:Key="KanbanContainerColor" Color="Transparent"/>
  <SolidColorBrush x:Key="MatrixContainerColor" Color="Transparent"/>
  <SolidColorBrush x:Key="TimelineContainerBackground" Color="#FFF2F6"/>
  <Color x:Key="TimelineContainerBackgroundStepToColor">#00FFF2F6</Color>
  <Color x:Key="TimelineContainerBackgroundColor">#FFF2F6</Color>
  <SolidColorBrush x:Key="ProjectSectionColor" Color="{DynamicResource ProjectMenuBaseColor}" Opacity="0.4"/>
</ResourceDictionary>