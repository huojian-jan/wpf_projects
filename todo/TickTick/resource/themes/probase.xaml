<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <SolidColorBrush x:Key="MainBackground" Color="Transparent"/>
  <SolidColorBrush x:Key="ProjectSelectedBackground" Color="{DynamicResource ColorPrimary}" Opacity="0.36"/>
  <SolidColorBrush x:Key="DateColorPrimary" Color="{DynamicResource ColorPrimary}"/>
  <SolidColorBrush x:Key="ShowProjectBackground" Color="{DynamicResource MainWindow}"/>
  <SolidColorBrush x:Key="ShowAreaBackground" Color="{DynamicResource white_90}"/>
</ResourceDictionary>